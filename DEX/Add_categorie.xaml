<Window x:Name="Add_categorie1" x:Class="DEX.Add_categorie"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="DEX - Adauga categorie" Height="136" Width="300" Icon="dictionar-icon.ico" ResizeMode="NoResize">
    <Grid>
        <TextBox x:Name="txt_new_categorie" Height="23" Margin="38,38,38,0" TextWrapping="Wrap" VerticalAlignment="Top"/>
        <TextBlock HorizontalAlignment="Left" Height="25" Margin="39,10,0,0" TextWrapping="Wrap" Text="Adaugati o noua categorie de cuvinte" VerticalAlignment="Top" Width="216" FontWeight="Bold"/>
        <Button x:Name="btn_ok_new_categorie" Content="Add" HorizontalAlignment="Left" Margin="50,70,0,0" VerticalAlignment="Top" Width="75" Click="btn_ok_new_categorie_Click"/>
        <Button x:Name="btn_cancel_new_categorie" Content="Cancel" Margin="169,70,0,0" VerticalAlignment="Top" HorizontalAlignment="Left" Width="75" Click="btn_cancel_new_categorie_Click"/>

    </Grid>
</Window>
